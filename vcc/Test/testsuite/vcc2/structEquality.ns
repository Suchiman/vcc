  struct A a1;
  struct A a2;
  int z;
  _(invariant \mine(&a1, &a2))
  _(invariant \deep_eq(a1, a2))
};

struct D {
  struct A a1;
  struct A a2;
  int z;
  _(invariant \mine(&a1))
  _(invariant \deep_eq(a1, a2))
};

void foo()
{
  struct A a, b;
