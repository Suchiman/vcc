
#include <vcc2.h>

ispure int foo(int i) ensures(result == 1) {
  return 1;
}
ispure unsigned int foo(unsigned int ui) ensures(result == 2) {
  return 2;
}

void bar() {
  int i = 0;
  unsigned int ui = 1;
  
  i = foo(i);
  ui = foo(ui);
  assert(i == 1);
  assert(ui == 2);
}
