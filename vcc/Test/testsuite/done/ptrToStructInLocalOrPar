#include <vcc.h>

struct A {
  int a,b,c,d;
};

struct B {
  int x;
  struct A a;
};

void foo()
{
  struct B *b;
  void *p;

  p = &b;
  assume(typed(b));
  assert(typed(&b->a));
}


void bar(struct B b)
{
  struct B *p;
  p = &b;
  assert(typed(p));
  assert(typed(&p->a));
}

`
Verification of foo succeeded.
Verification of bar succeeded.
`
