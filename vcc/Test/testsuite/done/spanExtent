
#include <vcc.h>

struct S {
  int a;
  int b;
};

struct T {
  int c;
  struct S s;
};

void foo(struct T *t)
{
  assert(set_in(&t->c, span(t)));
  assert(!set_in(&t->s, span(t)));
  assert(set_in(&t->s, extent(t)));
  assert(set_in(&t->s.a, extent(t)));
  assert(set_in(&t->s.b, extent(t)));
  assert(set_in(&t->s.a, span(&t->s)));
  assert(set_in(&t->s.b, span(&t->s)));
}

`
Verification of foo succeeded.
`
