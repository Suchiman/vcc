#include <vcc.h>

struct S {
  int a;
  int b;
};


struct vcc(dynamic_owns) T {
  struct S s1;
  struct S s2;
  invariant(set_eq(owns(this), SET(&s1, &s2)))
  invariant(s1.a == s2.a)
  invariant(s1.b == s2.b)
};

void foo(struct T t) { }

`
Verification of T#adm succeeded.
Verification of foo succeeded.
`
