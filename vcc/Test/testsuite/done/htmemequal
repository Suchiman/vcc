#include <vcc2.h>

typedef unsigned __int8 BOOLEAN;
typedef const void *PCVOID;
typedef unsigned __int64 size_t;
typedef size_t SIZE_T;


int
__cdecl
memcmp(
     const void* Buffer1,
     const void* Buffer2,
     size_t SizeInBytes
    );

BOOLEAN
HtEqualMemory(
     PCVOID Buffer1,
     PCVOID Buffer2,
     SIZE_T CountOfBytes
    )
{
    return (BOOLEAN)(memcmp(Buffer1, Buffer2, CountOfBytes) == 0);
}

