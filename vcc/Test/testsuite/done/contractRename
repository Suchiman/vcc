#include <vcc2.h>

void foo(int a) requires(a > 10);

void foo(int b) {
  assert(b > 0);
}
  
struct S {
  unsigned int x;
};


struct T {
  struct S s;
  __int64 y;
};
  
  
void bar(struct S s) requires(s.x > 10);

void bar(struct S s1)  {
  assert(s1.x > 10);
}

void baz(struct T t) 
  requires(t.s.x > 10)
  requires(t.y > 10);
  
void baz(struct T t1) {
  assert(t1.s.x > 10);
  assert(t1.y > 10);
}

`
Verification of foo succeeded.
Verification of bar succeeded.
Verification of baz succeeded.
`