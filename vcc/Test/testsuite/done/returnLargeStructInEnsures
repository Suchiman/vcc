#include <vcc2.h>

typedef unsigned __int64 UINT64;

struct Core
{
    UINT64   RIP;       // instruction pointer
    bool     GIF;       // Global Interrupt Flag (always 1 for INTEL)
};

ispure struct Core AFSvm(UINT64 rip);

UINT64 ValGetGpRegister(UINT64 rip)
  ensures(result == AFSvm(rip).RIP)
{
	return AFSvm(rip).RIP;
}

`
Verification of ValGetGpRegister succeeded.
`
