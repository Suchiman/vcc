#include <vcc.h>

void foo()
{
  
  spec(ptrset a, b;)
  void *x;

  assert(set_in(x,a) && !set_in(x,b) ==> set_in(x,set_difference(a,b)));
  assert(set_in(x,a) && set_in(x,b) ==> !set_in(x,set_difference(a,b)));
  assert(!set_in(x,a) && set_in(x,b) ==> !set_in(x,set_difference(a,b)));
  assert(!set_in(x,a) && !set_in(x,b) ==> !set_in(x,set_difference(a,b)));
}
`
Verification of foo succeeded.
`
#include "vcc.h"

void bar() {
	assert(set_eq(set_intersection(SET((void*)40),SET((void*)44)),SET()));
	assert(set_eq(set_intersection(SET((void*)40),SET((void*)40)),SET((void*)40)));
}

`
Verification of bar succeeded.
