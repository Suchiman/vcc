#include <vcc2.h>

__specification (bool f1(unsigned __int64 i);)

__specification (bool f2(unsigned __int64 j);)

__specification (bool f3(unsigned __int64 j);)

void foo() {
  assert(forall(unsigned __int64 i; f1(i) ==> f2(i) ((f3(i) || f3(i)))));
}

`
testcase(10,51) : error VC0000: 'f2(i)' is not a method and cannot be called.
`