#include <vcc.h>

typedef int (*foo)(int i)
  requires(i > 0)
  ensures(result > 0);

int test(int i)
  requires(i >= 0)
  ensures(result > 1)
{
  return 2;
}

int bar(foo fum)
  reads(fum)  requires(thread_local(fum))
{
  return fum(1);
}

int myn(){
  return bar((foo)&test);
}
`
Verification of test succeeded.
Verification of bar succeeded.
Verification of myn succeeded.
Verification of $fnptr_test_to_fnptr#1 succeeded.
`
