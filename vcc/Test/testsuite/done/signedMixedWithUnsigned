#include <vcc.h>

unsigned int foo() {
  unsigned int x = 5;
  return x & ~7;
}

unsigned short foo2() {
  unsigned short x = 5;
  return x | 3;
}

void foo3() {
  __int8 x = 10;
  unsigned __int8 y = 15;
  x &= y;
}


void foo4() {
  int x = 10;
  unsigned int y = 5;
  
  x = x / y;
}

void foo5() {
  __int64 x = 10;
  unsigned __int64 y = 5;
  
  x = x / y;
}


void main(){}
`
testcase(24,7) : error VC0000: Operator '/' cannot be applied to operands of type '__int32' and 'unsigned __int32'.
testcase(31,7) : error VC0000: Operator '/' cannot be applied to operands of type '__int64' and 'unsigned __int64'.
`
#include <vcc.h>

typedef unsigned long long U8;
typedef unsigned char U1;
U8 rcx;
U8* MSRs;

void RtlpZeroSinglePage()
  requires(program_entry_point())
  requires(is_thread_local_array(MSRs+0x0C0000080,8))
{
  assume(rcx == 0x0C0000080);
  assert(is_thread_local_array(MSRs+0x0C0000080,8));
  assert(is_thread_local_array(MSRs+rcx,8));
}
`
Verification of RtlpZeroSinglePage succeeded.
