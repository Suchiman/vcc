#include "vcc.h"

void foo() {
    spec(bool map[unsigned][unsigned];)
    spec(map = (lambda(unsigned i; true; (lambda(unsigned j; true; j==i))));)
    assert(map[3][3]);
    assert(!map[3][2]);
    assert(forall(unsigned i; map[i][i]));
    assert(forall(unsigned i,j; i!=j ==> !map[i][j]));
}
`
Verification of foo succeeded.
`
