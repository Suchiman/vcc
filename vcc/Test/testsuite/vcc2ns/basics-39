void foo(void *f) { 
  void *p = f + 3;
} 

void bar(void *f) { 
  void *p = f - 3;
} 

void baz(void *f) { 
  void *p = 3 + f;
} 

void qux(void *f) { 
  void *p = 100 - f;
} 
`
