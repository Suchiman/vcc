#include <vcc.h>

struct S {
  int x;
};

void bar(struct S *a, struct S *b)
  requires(thread_local(b))
  requires(typed(b))
  requires(a != b)
  writes(extent(a))
{
  a->x = 1;
  a->x = b->x;
}

void foo(int *a, int *b)
    requires(thread_local(b))
    requires(a != b)
    writes(a)
{
    *a = 5;
    *a = *b;
}
`
Verification of bar succeeded.
Verification of foo succeeded.
`
