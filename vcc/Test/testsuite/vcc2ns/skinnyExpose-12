#include <vcc.h>

struct A {
  int x;
  int y;
};

void up1(struct A *a)
  writes(a)
  maintains(wrapped(a))
  ensures(domain_updated_at(a, SET(&a->x, &a->y)))
{
  skinny_expose(a)
      writes(&a->y, &a->x)
  {
    a->x = 1;
    a->y = 1;
  }
}
`
Verification of up1 succeeded.
`
