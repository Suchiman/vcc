#include <vcc.h>

typedef int (*TF)(int x)
  requires(x > 1)
  ensures(result > 0);

int bar(int x)
  requires(x > 0)
  ensures(result > 1);

void foo() {
  TF p = &bar;
  int res = p(-1);
  assert(res > 0);
}
`
