#include <vcc.h>

struct S{

  def_group(G0)
  def_group(G1)
  int a;
  def_group(G)
  int b;
  int c;
  def_group(G2)
};

struct T {
  int a;
  int b;
  int c;
};

void foo(struct S *s) {
  assert(&(s->b) - &(s->a) == &(s->c) - &(s->b));
}

void bar(struct T *t) {
  size_t ss = sizeof(struct S);
  size_t st = sizeof(struct T);
  assert(ss == st);
}
`
Verification of foo succeeded.
Verification of bar succeeded.
`
