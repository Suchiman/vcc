#include "vcc2test.h"


void setk3(int *p, size_t sz)
  requires(is_thread_local_array(p, sz))
  writes(array_range(p, sz))
{
  size_t i;
  int *x;

  i = 0;
  while (i < sz) 
    writes(array_range(p, sz))
    invariant (i >= 0)
  {
    p[i] = unchecked((int)i);
    i = i + 1;
    if(0) free(x);
  }
}

`
Verification of setk3 succeeded.
`
