#include <vcc.h>

struct B {
  int x;
  int y;
};
struct vcc(dynamic_owns) A {
  int z;
  struct B *b;
  invariant(keeps(b))
};

void foo(struct A *a)
  maintains(wrapped(a))
  writes(a)
  ensures(domain_updated_at(a, SET(&a->b->x)))
{
  skinny_expose(a, a->b)
    writes(&a->b->x)
  {
    a->b->x = 12;
  }
}

`
Verification of A#adm succeeded.
Verification of foo succeeded.
`
