#include "vcc.h"

spec(ispure bool foo(int *a)
    reads(set_universe())
    returns(*a);)

spec(ispure bool bar(int ^a)
    reads(set_universe())
    returns(*a);)

void wrong_type() {
    assert(foo(false));
    assert(foo((int *)false));
    assert(bar(false));
    assert(bar((int ^)false));
}
`
