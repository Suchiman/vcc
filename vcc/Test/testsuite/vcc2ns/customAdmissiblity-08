#include <vcc.h>

struct A {
  int x;
  invariant(this->x>0)
};


spec(isadmissibilitycheck
void adm_A(struct A* x)
{
  havoc_others(x);
})

void qux()
{
  spec( adm_A((struct A*)0); )
}
`
