#include "vcc.h"

void success1()
{
	spec(int a[int][int];)
	spec(int b[int];)
	spec(int foo[int];)
spec(
	foo = lambda(int x; x > 0 && x<100; x*x);
	a = lambda(int y; y>0; foo);
	foo = lambda(int x; x > 0 && x<100; x*x*x);
	a[6] = foo;
	b = a[5];
	assert(b[3] == 9);
	b = a[6];
	assert(b[3] == 27);
)
}
`
Verification of success1 succeeded.
`
