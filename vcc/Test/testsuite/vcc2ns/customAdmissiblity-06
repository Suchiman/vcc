#include <vcc.h>

struct A {
  int x;
  
  invariant(this->x > 0)
};

spec(isadmissibilitycheck
void adm_A(struct A* x)
  ensures(false)
{
  havoc_others(x);
  assert(x->x > 0);
})
`
testcase(9,17) : error VC9624: custom admissibility checks are not allowed to have explicit requires/ensures/writes
`
