#include "vcc.h"

typedef unsigned __int64 uint64_t;

typedef int (*F)(int);
typedef unsigned (*G)(unsigned);

int foo(int a);

int baz(F f) 
  requires(typed(f))
{
  f(4);
}

int bar() {
  F x = foo;
  G y = unchecked((G)x);
  baz(unchecked((F)y));
}
`
Verification of baz succeeded.
Verification of bar succeeded.
Verification of $fnptr_foo_to_fnptr#1 succeeded.
`
