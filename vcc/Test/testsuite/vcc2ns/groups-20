#include "vcc.h"

typedef struct _TEST{
  int a;
  def_group(gMisc)
  in_group(gMisc) int b;
  inv_group(gMisc, keeps((struct _TEST*)this))
} TEST, *PTEST;

void Init(PTEST inp)
  writes(extent(inp))
  ensures(wrapped(inp::gMisc))
{
  inp->a = 0;
  inp->b = 1;
  wrap(inp);
  wrap(inp::gMisc);
}; 
`
Verification of _TEST##gMisc#adm succeeded.
Verification of Init succeeded.
`
